{
  "name": "react-starter",
  "author": "Etienne Bondot <etiennebondot@gmail.com>",
  "private": true,
  "version": "1.0.0",
  "scripts": {
    "build": "webpack --config webpack/webpack.dev.js",
    "build:prod": "webpack --config webpack/webpack.prod.js",
    "start:watch": "webpack-dev-server --config webpack/webpack.dev.hot.js",
    "storybook": "start-storybook",
    "test": "concurrently \"yarn test:jest\" \"yarn test:linter\"",
    "test:coverage": "concurrently \"yarn test:jest:coverage\" \"yarn test:linter\"",
    "test:cypress": "cypress run",
    "test:jest": "jest src/**/__tests__/*.test.js",
    "test:jest:coverage": "yarn run test:jest -- --coverage --collectCoverageFrom='[\"src/**/*.{ts,tsx}\",\"!src/**/stories/*\"]'",
    "test:linter": "eslint --cache 'src/app/**/*.tsx'"
  },
  "husky": {
    "hooks": {
      "pre-commit": "yarn test"
    }
  },
  "dependencies": {
    "clean-webpack-plugin": "^3.0.0",
    "copy-webpack-plugin": "^6.1.0",
    "html-webpack-plugin": "^4.4.1",
    "react": "^16.13.1",
    "react-dom": "^16.9.0",
    "react-router": "^5.2.0",
    "react-router-dom": "^5.2.0",
    "recompose": "^0.30.0"
  },
  "devDependencies": {
    "@babel/core": "^7.11.5",
    "@babel/plugin-proposal-class-properties": "^7.10.4",
    "@babel/plugin-proposal-object-rest-spread": "^7.11.0",
    "@babel/preset-env": "^7.11.5",
    "@babel/preset-react": "^7.10.4",
    "@babel/preset-typescript": "^7.10.4",
    "@storybook/addon-storyshots": "^6.0.21",
    "@storybook/react": "^6.0.21",
    "@types/react": "^16.9.49",
    "@types/react-dom": "^16.9.8",
    "@types/react-router-dom": "^5.1.5",
    "@types/webpack-dev-server": "^3.1.6",
    "@types/webpack-env": "^1.13.9",
    "@typescript-eslint/eslint-plugin": "^2.3.0",
    "@typescript-eslint/parser": "^2.3.0",
    "babel-jest": "^24.3.1",
    "babel-loader": "^8.0.5",
    "babel-plugin-macros": "^2.5.0",
    "babel-plugin-module-resolver": "^3.1.3",
    "babel-polyfill": "^6.26.0",
    "concurrently": "^4.1.0",
    "css-loader": "^2.1.1",
    "cssnano": "^4.1.10",
    "cypress": "^3.2.0",
    "eslint": "^6.4.0",
    "eslint-config-prettier": "^6.3.0",
    "eslint-config-react": "^1.1.7",
    "eslint-plugin-prettier": "^3.1.1",
    "eslint-plugin-react": "^7.14.3",
    "file-loader": "^3.0.1",
    "husky": "^1.3.1",
    "jest": "^24.3.1",
    "mini-css-extract-plugin": "^0.5.0",
    "node-sass": "^4.11.0",
    "postcss-loader": "^3.0.0",
    "postcss-preset-env": "^6.6.0",
    "prettier": "^1.18.2",
    "react-hot-loader": "^4.6.3",
    "react-test-renderer": "^16.8.4",
    "require-context.macro": "^1.0.4",
    "sass-loader": "^7.1.0",
    "source-map-loader": "^0.2.4",
    "style-loader": "^0.23.1",
    "terser-webpack-plugin": "^1.2.3",
    "ts-loader": "^6.0.4",
    "typescript": "^4.0.2",
    "webpack": "^4.29.0",
    "webpack-cli": "^3.2.1",
    "webpack-dev-server": "^3.1.14",
    "webpack-merge": "^4.2.1"
  }
}
